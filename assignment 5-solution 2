

function getGrade(marks){
  var grade;
  var score = parseInt(marks/10);
 
  switch (score) {
    case 10:
    case 9:
    case 8:
        grade ="A"
        break;
    case 7:
        grade ="B"
        break;
    case 6:
        grade="C"
        break;
    case 5:
        grade="D"
        break;
    case 4:
    case 3:
    case 2:
    case 1:
    case 0:
        grade="E"
        break;
    default:
        grade="F"
      
      }
  return grade;
}
console.log(getGrade(48))
